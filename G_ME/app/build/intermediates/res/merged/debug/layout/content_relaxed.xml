<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.deepika.sampangi.g_me.relaxed"
    tools:showIn="@layout/activity_relaxed"
    android:background="#0dbb7f">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView4"
        android:fillViewport="false">



        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">





            <ImageButton
                android:layout_width="308dp"
                android:layout_height="131dp"
                android:id="@+id/rbtn1"
                android:src="@drawable/shoppingicon"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/angryfbtn"
                android:layout_toEndOf="@+id/angryfbtn"
                android:background="#47eba6" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="SHOPPING"
                android:id="@+id/textView59" />

            <ImageButton
                android:layout_width="354dp"
                android:layout_height="150dp"
                android:id="@+id/rbtn2"
                android:src="@drawable/target"
                android:layout_alignTop="@+id/angryfbtn"
                android:layout_toRightOf="@+id/angryfbtn"
                android:layout_toEndOf="@+id/angryfbtn"
                android:layout_row="0"
                android:layout_column="2"
                android:background="#47eba6" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="SET GOALS"
                android:id="@+id/textView60" />

            <ImageButton
                android:layout_width="348dp"
                android:layout_height="156dp"
                android:id="@+id/rbtn3"
                android:src="@drawable/whoami"



                android:background="#47eba6"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="KNOW FACTS"
                android:id="@+id/textView61" />

            <ImageButton
                android:layout_width="348dp"
                android:layout_height="156dp"
                android:id="@+id/todo"
                android:src="@drawable/notepad"



                android:background="#47eba6"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="LIST OUT ACTIVITIES"
                android:id="@+id/textView62" />




        </TableLayout>

    </ScrollView>

</RelativeLayout>
